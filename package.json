{
  "name": "titmouse",
  "version": "0.0.1",
  "description": "titmouse",
  "type": "module",
  "bin": {
    "titmouse": "./dist/cli.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun build ./src/cli.ts --outdir ./dist --target node && bun build ./src/index.ts --outdir ./dist --target node && tsc --emitDeclarationOnly",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "oxlint ./src",
    "check": "tsc --noEmit",
    "prepublishOnly": "bun run build"
  },
  "devDependencies": {
    "typescript": "^5.7.0",
    "vitest": "^3.0.0",
    "oxlint": "^0.15.0",
    "fast-check": "^3.0.0",
    "@types/bun": "^1.2.0"
  },
  "keywords": [],
  "license": "MIT"
}
